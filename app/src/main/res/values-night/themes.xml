<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.SMSExporter" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/CP</item>
        <item name="colorPrimaryVariant">@color/CPV</item>
        <item name="colorOnPrimary">@color/CPO</item>
        <item name="colorSecondary">@color/CS</item>
        <item name="colorSecondaryVariant">@color/CSV</item>
        <item name="colorOnSecondary">@color/CSO</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorSecondary</item>
        <item name="android:windowBackground">?attr/colorSecondary</item>
        <item name="android:windowLightStatusBar" tools:ignore="NewApi">false</item>
        <item name="android:navigationBarColor" tools:ignore="NewApi">@color/CSV</item>
        <item name="android:windowLightNavigationBar" tools:targetApi="o_mr1">false</item>
        <item name="popupMenuStyle">@style/Widget.MaterialComponents.PopupMenu</item>
        <item name="android:contextPopupMenuStyle" tools:ignore="NewApi">
            @style/Widget.MaterialComponents.PopupMenu.ContextMenu
        </item>
        <item name="actionOverflowMenuStyle">
            @style/Widget.MaterialComponents.PopupMenu.Overflow
        </item>
    </style>
</resources>