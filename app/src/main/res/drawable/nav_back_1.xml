<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/TP" />
            <size
                android:width="100dp"
                android:height="100dp" />
        </shape>
    </item>
    <item
        android:bottom="-1dp"
        android:left="65dp"
        android:right="10dp"
        android:top="16dp">
        <rotate
            android:fromDegrees="53"
            android:pivotX="100%"
            android:pivotY="0%">
            <shape android:shape="rectangle">
                <solid android:color="@color/nav_back_1" />
                <corners android:radius="20dp" />
            </shape>
        </rotate>
    </item>
    <item
        android:bottom="16dp"
        android:left="65dp"
        android:right="10dp"
        android:top="-1dp">
        <rotate
            android:fromDegrees="-53"
            android:pivotX="100%"
            android:pivotY="100%">
            <shape android:shape="rectangle">
                <solid android:color="@color/nav_back_1" />
                <corners android:radius="20dp" />
            </shape>
        </rotate>
    </item>
</layer-list>